:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x14ff0af60 .scope module, "hcsr04_sensor_tb" "hcsr04_sensor_tb" 2 3;
 .timescale -9 -12;
v0x14ff28750_0 .var "clk", 0 0;
v0x14ff287e0_0 .net "distance", 15 0, v0x14ff280e0_0;  1 drivers
v0x14ff28870_0 .net "done", 0 0, v0x14ff281a0_0;  1 drivers
v0x14ff28940_0 .var "echo", 0 0;
v0x14ff289f0_0 .var "start", 0 0;
v0x14ff28ac0_0 .net "trigger", 0 0, v0x14ff28620_0;  1 drivers
E_0x14ff0c970 .event anyedge, v0x14ff281a0_0;
S_0x14ff0b0d0 .scope task, "simulate_distance" "simulate_distance" 2 32, 2 32 0, S_0x14ff0af60;
 .timescale -9 -12;
v0x14ff18090_0 .var/i "distance_mm", 31 0;
v0x14ff279d0_0 .var "echo_cycles", 31 0;
E_0x14ff0b650 .event anyedge, v0x14ff28620_0;
TD_hcsr04_sensor_tb.simulate_distance ;
    %load/vec4 v0x14ff18090_0;
    %muli 294, 0, 32;
    %store/vec4 v0x14ff279d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x14ff28ac0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.1, 6;
    %wait E_0x14ff0b650;
    %jmp T_0.0;
T_0.1 ;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14ff28940_0, 0, 1;
    %load/vec4 v0x14ff279d0_0;
    %muli 20, 0, 32;
    %pad/u 64;
    %muli 1000, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff28940_0, 0, 1;
    %end;
S_0x14ff27a70 .scope module, "uut" "hcsr04_sensor" 2 12, 3 1 0, S_0x14ff0af60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "start";
    .port_info 2 /OUTPUT 1 "trigger";
    .port_info 3 /INPUT 1 "echo";
    .port_info 4 /OUTPUT 1 "done";
    .port_info 5 /OUTPUT 16 "distance";
P_0x14ff27c30 .param/l "IDLE" 1 3 11, C4<00>;
P_0x14ff27c70 .param/l "MEASURE" 1 3 14, C4<11>;
P_0x14ff27cb0 .param/l "TRIGGER" 1 3 12, C4<01>;
P_0x14ff27cf0 .param/l "WAIT_ECHO" 1 3 13, C4<10>;
v0x14ff27f80_0 .net "clk", 0 0, v0x14ff28750_0;  1 drivers
v0x14ff28030_0 .var "counter", 9 0;
v0x14ff280e0_0 .var "distance", 15 0;
v0x14ff281a0_0 .var "done", 0 0;
v0x14ff28240_0 .net "echo", 0 0, v0x14ff28940_0;  1 drivers
v0x14ff28320_0 .var "echo_count", 15 0;
v0x14ff283d0_0 .var "measuring", 0 0;
v0x14ff28470_0 .net "start", 0 0, v0x14ff289f0_0;  1 drivers
v0x14ff28510_0 .var "state", 1 0;
v0x14ff28620_0 .var "trigger", 0 0;
E_0x14ff27f30 .event posedge, v0x14ff27f80_0;
    .scope S_0x14ff27a70;
T_1 ;
    %wait E_0x14ff27f30;
    %load/vec4 v0x14ff28510_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %jmp T_1.4;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14ff28620_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14ff281a0_0, 0;
    %load/vec4 v0x14ff28470_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x14ff28510_0, 0;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x14ff28030_0, 0;
T_1.5 ;
    %jmp T_1.4;
T_1.1 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x14ff28620_0, 0;
    %load/vec4 v0x14ff28030_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x14ff28030_0, 0;
    %load/vec4 v0x14ff28030_0;
    %pad/u 32;
    %cmpi/u 499, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.7, 5;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x14ff28510_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14ff28620_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14ff283d0_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x14ff28320_0, 0;
T_1.7 ;
    %jmp T_1.4;
T_1.2 ;
    %load/vec4 v0x14ff28240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.9, 8;
    %pushi/vec4 3, 0, 2;
    %assign/vec4 v0x14ff28510_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x14ff283d0_0, 0;
T_1.9 ;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x14ff283d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.11, 8;
    %load/vec4 v0x14ff28240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.13, 8;
    %load/vec4 v0x14ff28320_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x14ff28320_0, 0;
    %jmp T_1.14;
T_1.13 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x14ff283d0_0, 0;
    %load/vec4 v0x14ff28320_0;
    %pad/u 32;
    %pushi/vec4 294, 0, 32;
    %div;
    %pad/u 16;
    %assign/vec4 v0x14ff280e0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x14ff281a0_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x14ff28510_0, 0;
T_1.14 ;
T_1.11 ;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x14ff0af60;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff28750_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff289f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff28940_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x14ff0af60;
T_3 ;
    %delay 10000, 0;
    %load/vec4 v0x14ff28750_0;
    %inv;
    %store/vec4 v0x14ff28750_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x14ff0af60;
T_4 ;
    %vpi_call 2 26 "$dumpfile", "hcsr04_wave.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, v0x14ff28750_0, v0x14ff289f0_0, v0x14ff28ac0_0, v0x14ff28940_0, v0x14ff28870_0, v0x14ff287e0_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x14ff0af60;
T_5 ;
    %vpi_call 2 50 "$display", "Starting HC-SR04 sensor simulation..." {0 0 0};
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14ff289f0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff289f0_0, 0, 1;
    %pushi/vec4 100, 0, 32;
    %store/vec4 v0x14ff18090_0, 0, 32;
    %fork TD_hcsr04_sensor_tb.simulate_distance, S_0x14ff0b0d0;
    %join;
T_5.0 ;
    %load/vec4 v0x14ff28870_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_5.1, 6;
    %wait E_0x14ff0c970;
    %jmp T_5.0;
T_5.1 ;
    %vpi_call 2 58 "$display", "Distance measured: %d mm", v0x14ff287e0_0 {0 0 0};
    %delay 1000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14ff289f0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ff289f0_0, 0, 1;
    %pushi/vec4 500, 0, 32;
    %store/vec4 v0x14ff18090_0, 0, 32;
    %fork TD_hcsr04_sensor_tb.simulate_distance, S_0x14ff0b0d0;
