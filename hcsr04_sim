#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x12b60d600 .scope module, "hcsr04_sensor_tb" "hcsr04_sensor_tb" 2 3;
 .timescale -9 -12;
v0x12b62f020_0 .var "clk", 0 0;
v0x12b62f0b0_0 .net "distance", 15 0, v0x12b62e9b0_0;  1 drivers
v0x12b62f140_0 .net "done", 0 0, v0x12b62ea70_0;  1 drivers
v0x12b62f210_0 .var "echo", 0 0;
v0x12b62f2c0_0 .var "start", 0 0;
v0x12b62f390_0 .net "trigger", 0 0, v0x12b62eef0_0;  1 drivers
E_0x12b60eff0 .event anyedge, v0x12b62ea70_0;
S_0x12b60d770 .scope task, "simulate_distance" "simulate_distance" 2 32, 2 32 0, S_0x12b60d600;
 .timescale -9 -12;
v0x12b61a960_0 .var/i "distance_mm", 31 0;
v0x12b62e2a0_0 .var "echo_cycles", 31 0;
E_0x12b60dcd0 .event anyedge, v0x12b62eef0_0;
TD_hcsr04_sensor_tb.simulate_distance ;
    %load/vec4 v0x12b61a960_0;
    %muli 294, 0, 32;
    %store/vec4 v0x12b62e2a0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x12b62f390_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.1, 6;
    %wait E_0x12b60dcd0;
    %jmp T_0.0;
T_0.1 ;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12b62f210_0, 0, 1;
    %load/vec4 v0x12b62e2a0_0;
    %pad/u 64;
    %muli 1000, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f210_0, 0, 1;
    %end;
S_0x12b62e340 .scope module, "uut" "hcsr04_sensor" 2 12, 3 1 0, S_0x12b60d600;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "start";
    .port_info 2 /OUTPUT 1 "trigger";
    .port_info 3 /INPUT 1 "echo";
    .port_info 4 /OUTPUT 1 "done";
    .port_info 5 /OUTPUT 16 "distance";
P_0x12b62e500 .param/l "IDLE" 1 3 11, C4<00>;
P_0x12b62e540 .param/l "MEASURE" 1 3 14, C4<11>;
P_0x12b62e580 .param/l "TRIGGER" 1 3 12, C4<01>;
P_0x12b62e5c0 .param/l "WAIT_ECHO" 1 3 13, C4<10>;
v0x12b62e850_0 .net "clk", 0 0, v0x12b62f020_0;  1 drivers
v0x12b62e900_0 .var "counter", 9 0;
v0x12b62e9b0_0 .var "distance", 15 0;
v0x12b62ea70_0 .var "done", 0 0;
v0x12b62eb10_0 .net "echo", 0 0, v0x12b62f210_0;  1 drivers
v0x12b62ebf0_0 .var "echo_count", 15 0;
v0x12b62eca0_0 .var "measuring", 0 0;
v0x12b62ed40_0 .net "start", 0 0, v0x12b62f2c0_0;  1 drivers
v0x12b62ede0_0 .var "state", 1 0;
v0x12b62eef0_0 .var "trigger", 0 0;
E_0x12b62e800 .event posedge, v0x12b62e850_0;
    .scope S_0x12b62e340;
T_1 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x12b62ede0_0, 0, 2;
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x12b62e900_0, 0, 10;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x12b62ebf0_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62eca0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x12b62e340;
T_2 ;
    %wait E_0x12b62e800;
    %load/vec4 v0x12b62ede0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %jmp T_2.4;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12b62eef0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12b62ea70_0, 0;
    %load/vec4 v0x12b62ed40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.5, 8;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x12b62ede0_0, 0;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x12b62e900_0, 0;
T_2.5 ;
    %jmp T_2.4;
T_2.1 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x12b62eef0_0, 0;
    %load/vec4 v0x12b62e900_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x12b62e900_0, 0;
    %load/vec4 v0x12b62e900_0;
    %pad/u 32;
    %cmpi/u 499, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_2.7, 5;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x12b62ede0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12b62eef0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12b62eca0_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x12b62ebf0_0, 0;
T_2.7 ;
    %jmp T_2.4;
T_2.2 ;
    %load/vec4 v0x12b62eb10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.9, 8;
    %pushi/vec4 3, 0, 2;
    %assign/vec4 v0x12b62ede0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x12b62eca0_0, 0;
T_2.9 ;
    %jmp T_2.4;
T_2.3 ;
    %load/vec4 v0x12b62eca0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.11, 8;
    %load/vec4 v0x12b62eb10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.13, 8;
    %load/vec4 v0x12b62ebf0_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x12b62ebf0_0, 0;
    %jmp T_2.14;
T_2.13 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12b62eca0_0, 0;
    %load/vec4 v0x12b62ebf0_0;
    %pad/u 32;
    %muli 100, 0, 32;
    %pushi/vec4 294, 0, 32;
    %div;
    %pad/u 16;
    %assign/vec4 v0x12b62e9b0_0, 0;
    %load/vec4 v0x12b62ebf0_0;
    %pad/u 32;
    %muli 100, 0, 32;
    %pushi/vec4 294, 0, 32;
    %div;
    %vpi_call 3 65 "$display", "Debug: echo_count=%d, calculated distance=%d", v0x12b62ebf0_0, S<0,vec4,u32> {1 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x12b62ea70_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x12b62ede0_0, 0;
T_2.14 ;
T_2.11 ;
    %jmp T_2.4;
T_2.4 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x12b60d600;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f020_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f2c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f210_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x12b60d600;
T_4 ;
    %delay 10000, 0;
    %load/vec4 v0x12b62f020_0;
    %inv;
    %store/vec4 v0x12b62f020_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x12b60d600;
T_5 ;
    %vpi_call 2 26 "$dumpfile", "hcsr04_wave.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, v0x12b62f020_0, v0x12b62f2c0_0, v0x12b62f390_0, v0x12b62f210_0, v0x12b62f140_0, v0x12b62f0b0_0 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x12b60d600;
T_6 ;
    %vpi_call 2 50 "$display", "Starting HC-SR04 sensor simulation..." {0 0 0};
    %delay 100000, 0;
    %vpi_call 2 54 "$display", "Starting test case 1: 100mm" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12b62f2c0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f2c0_0, 0, 1;
    %vpi_call 2 57 "$display", "Waiting for trigger..." {0 0 0};
T_6.0 ;
    %load/vec4 v0x12b62f390_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_6.1, 6;
    %wait E_0x12b60dcd0;
    %jmp T_6.0;
T_6.1 ;
    %vpi_call 2 59 "$display", "Trigger complete, simulating echo..." {0 0 0};
    %pushi/vec4 100, 0, 32;
    %store/vec4 v0x12b61a960_0, 0, 32;
    %fork TD_hcsr04_sensor_tb.simulate_distance, S_0x12b60d770;
    %join;
    %vpi_call 2 61 "$display", "Waiting for done signal..." {0 0 0};
T_6.2 ;
    %load/vec4 v0x12b62f140_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_6.3, 6;
    %wait E_0x12b60eff0;
    %jmp T_6.2;
T_6.3 ;
    %vpi_call 2 63 "$display", "Distance measured: %d mm", v0x12b62f0b0_0 {0 0 0};
    %delay 1000000, 0;
    %vpi_call 2 67 "$display", "Starting test case 2: 500mm" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12b62f2c0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12b62f2c0_0, 0, 1;
    %vpi_call 2 70 "$display", "Waiting for trigger..." {0 0 0};
T_6.4 ;
    %load/vec4 v0x12b62f390_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_6.5, 6;
    %wait E_0x12b60dcd0;
    %jmp T_6.4;
T_6.5 ;
    %vpi_call 2 72 "$display", "Trigger complete, simulating echo..." {0 0 0};
    %pushi/vec4 500, 0, 32;
    %store/vec4 v0x12b61a960_0, 0, 32;
