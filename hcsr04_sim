T_6.7 ;
    %vpi_call 2 76 "$display", "Distance measured: %d mm", v0x12b62f0b0_0 {0 0 0};
    %delay 1000000, 0;
    %vpi_call 2 80 "$display", "Starting test case 3: 1000mm" {0 0 0};
    %pushi/vec4 1, 0, 1;
